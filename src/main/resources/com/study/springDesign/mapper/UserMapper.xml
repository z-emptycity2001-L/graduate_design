<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.study.springDesign.mapper.UserMapper">
    <insert id="insertUser">
        insert into user values (#{id},#{nickname},#{phone},#{sex},#{avatar},#{address},#{wechatName},#{createTime},#{updateTime})
    </insert>
</mapper>












